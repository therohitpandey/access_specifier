#include <iostream.h>
#include<conio.h>
using namespace std;

class base
{
 	private:
        int x;

 	protected:
 	    int y;

 	public:
 	    int z;

 	base() 
 	{
 	   x = 1;
 	   y = 2;
 	   z = 3;
 	}
};

class derive: protected base
{
 
 	public:
 	    void showdata()
 	    {
 	       cout << "x is not accessible" << endl;
               cout << "value of y is " << y << endl;
               cout << "value of z is " << z << endl;
 	    }
};
int main()
{
     derive a; 
     a.showdata();
   
    
  
     return 0;
} 	
